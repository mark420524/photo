.item{
  box-sizing: border-box;
  padding: 10rpx;
  width: 100%;
  padding-bottom: 30rpx;
  margin: 0rpx;
  border-radius: 0rpx;
  margin-left: 30rpx;
}
.name{
  font-size: 30rpx;
    font-weight: bold;
    color: black;
    line-height: 100rpx;
    padding: 0 30rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;

}
.size{
  display: flex;
  padding: 0 30rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 24rpx;
}